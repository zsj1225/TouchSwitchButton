<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TouchSwitchButton">
        <attr name="tsb_thumb" format="reference" />
        <attr name="tsb_thumbId" format="reference" />
        <attr name="tsb_direction">
            <flag name="TwoWay" value="0" />
            <flag name="ToLeft" value="1" />
            <flag name="ToRight" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RingView">
        <attr name="ring_width" format="dimension"/>
        <attr name="ring_color" format="color"/>
    </declare-styleable>
</resources>